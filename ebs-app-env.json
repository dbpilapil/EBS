{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "test",
  "Parameters": {
    "AppVersion": {
      "Type": "String",
      "Description": "VersionLabel"
    },
    "EnvironmentName": {
      "Type": "String",
      "Default":"DEV01"
    },
    "VPCId": {
      "Type": "String",
      "Default":"vpc-111"
    },
    "PrivateSubnets": {
      "Type": "String",
      "Default":"subnet-123,subnet-1234"
    },
    "ApplicationName": {
      "Type": "String",
      "Default": "ApplicationName"
    }
  },
  "Resources": {
    "SampleApplication": {
      "Type": "AWS::ElasticBeanstalk::Application",
      "Properties": {
        "Description": "SampleApplication",
        "ApplicationName": { 
          "Ref": "ApplicationName"
        }
      }
    },
     "SampleEnvironment": {
      "Type": "AWS::ElasticBeanstalk::Environment",
    
      "Properties": {
        "ApplicationName": {"Ref":"SampleApplication"},
        "EnvironmentName": {"Ref": "EnvironmentName"},
        "Description": "SampleEnvironment",
        "VersionLabel": {
          "Ref": "AppVersion"
        },
        "Tier": {
          "Name": "WebServer",
          "Type": "Standard"
        },
        "TemplateName": {
          "Ref": "ApplicationName"
        },
        "OptionSettings": [
          {
            "Namespace": "aws:elasticbeanstalk:application:environment",
            "OptionName": "AWS_REGION",
            "Value": "us-east-1"
         }
          
        ]
      }
    }
 
  }
}
